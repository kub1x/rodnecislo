{
  "name": "rodnecislo",
  "version": "1.6.0",
  "description": "A package for handling Czech and Slovak Personal ID",
  "main": "lib/rodnecislo.mjs",
  "types": "lib/rodnecislo.d.ts",
  "type": "module",
  "files": [
    "lib/"
  ],
  "keywords": [
    "personal",
    "ID",
    "Czech",
    "node",
    "module"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kub1x/rodnecislo"
  },
  "scripts": {
    "audit": "$npm_execpath audit",
    "watch": "babel ./src --out-dir ./ --copy-files --watch",
    "build": "babel ./src --out-dir ./ --copy-files",
    "outdated": "$npm_execpath outdated --depth 0",
    "coverage": "nyc --reporter=lcov --reporter=text --reporter=html $npm_execpath test",
    "lint": "eslint src",
    "test": "ava",
    "test:watch": "ava --watch",
    "typescript-test": "tsc --project test",
    "major": "pmm major",
    "minor": "pmm minor",
    "patch": "pmm patch",
    "prepare": "husky"
  },
  "author": {
    "name": "Jakub Podlaha",
    "email": "j.podlaha@gmail.com",
    "url": "http://kub1x.org"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/register": "^7.28.3",
    "ava": "^6.4.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^9.36.0",
    "eslint-plugin-jsdoc": "^60.1.1",
    "husky": "^9.1.7",
    "nyc": "^17.1.0",
    "pmm": "^2.0.0",
    "sinon": "^21.0.0",
    "typescript": "^5.9.2"
  },
  "ava": {
    "files": [
      "test/*.mjs"
    ],
    "require": "@babel/register"
  }
}
